# Misc
alias nv="nvim ."
alias refresh="source ~/.zshrc"
alias lg="lazygit"
alias pdf="mupdf"
alias cl="clear"

# Better listing
alias ls="ls --color=auto"
alias ll="ls -l --color=auto"
alias la="ls -a --color=auto"

# Tmux
alias tn="tmux new"
alias ta="tmux attach"
alias td="tmux detach"
alias tl="tmux ls"
